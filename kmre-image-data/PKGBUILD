# Maintainer: sukanka <su975853527@gmail.com>
pkgname=kmre-image-data
pkgver=2.0_210819.11
pkgrel=1
pkgdesc='Ubuntu Kylin KMRE, an android container.'
arch=('any')
license=('Apache-2.0')
url='https://gitee.com/openkylin/kylin-kmre-image-data'
provides=("$pkgname")
groups=(kmre)
source=(
    git+${url}.git
    git+${url}-x64.git
)
makedepends=(git)
sha512sums=('SKIP' 'SKIP')

package() {

    #package
    cd $srcdir/kylin-${pkgname}
    install -Dm644 data/kmre.conf ${pkgdir}/usr/share/kmre/kmre.conf
}
